# This defines the maximal version of Java bytecode
# to make sure it runs on older JVM
# Byte code numbers:
#  45-49 -> Java1
#     50 -> Java6
#     51 -> Java7
#     52 -> Java8
#     53 -> Java9
#     54 -> Java10
#     55 -> Java11 - Current max version in Debian unstable
#     56 -> Java12
max-bytecode-version = 55

max-bytecode-existing-version = 56

# The following should not change
min-bytecode-version = 45
